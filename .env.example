# Database
DATABASE_URL=postgresql://user:password@localhost:5432/myturnyourturn

# OAuth Providers
# Google OAuth (https://console.cloud.google.com/)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Meta OAuth (https://developers.facebook.com/)
META_CLIENT_ID=your_meta_client_id_here
META_CLIENT_SECRET=your_meta_client_secret_here

# Apple OAuth (https://developer.apple.com/)
APPLE_CLIENT_ID=your_apple_client_id_here
